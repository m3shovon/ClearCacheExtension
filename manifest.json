{
  "manifest_version": 3,
  "name": "Smart Cache & Cookie Cleaner",
  "version": "1.0",
  "description": "Clears cache and cookies for the current website only.",
  "permissions": [
    "browsingData",
    "tabs",
    "cookies",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
